                        </ol>
                    </div>

                    <!--//sub-heard-part-->
                    <div class="graph-visual tables-main">

                        <!-- FORMULARIO -->
                        <form method="post">

                            <h3 class="inner-tittle two">Listar Requisições</h3>

                            <p></p>

                            <p>@(ViewBag.Mensagem) </p>

                            <p></p>



                            <div class="graph">

                                <div class="tables">
                                    <table class="table table-hover">
                                        <thead>
                                            <tr>
                                                <th>Codigo Requisição</th>
                                                <th>NIF</th>
                                                <th>Tipo de Requisição</th>
                                                <th>Status</th>
                                                <th>Data de Criação</th>
                                                <th>Detalhes</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @foreach (var R in ViewBag.ListaRequisicoesAprovadas)
                                            {
                                                <tr>
                                                    <td class="tdList">@(R.CodigoRequisicao)        </td>
                                                    <td class="tdList">@(R.FK_NIFUsuario)           </td>
                                                    <td class="tdList">@(R.NomeTipoRequisicao)      </td>
                                                    <td class="tdList">@(R.NomeStatus)              </td>
                                                    <td class="tdList">@(R.DataRequisicao)          </td>
                                                    <td class="tdIcon">
                                                        <a href="/Requisicao/HistoricoDetalhes/@(R.CodigoRequisicao)">
                                                            <i style="font-size: 14pt" class="glyphicon glyphicon-align-justify" title="Detalhes"></i>
                                                        </a>
                                                    </td>
                                                </tr>
                                            }
                                        </tbody>
                                    </table>
                                </div>
                            </div>


                        </form>
                    </div>
