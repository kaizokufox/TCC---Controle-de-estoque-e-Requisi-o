                                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" aria-expanded="false"><i class="fa fa-bell-o"></i> <span class="badge">@(Session["NotificacaoCoordenador"])</span></a>

                                        <ul class="dropdown-menu two">

                                            @if (Session["NotificacaoCoordenador"].Equals(0))
